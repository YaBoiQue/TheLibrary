@model IEnumerable<TheWarehouse.Models.Menuitem>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedTs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedTs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Menucategory)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedTs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedTs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Menucategory.MenucategoryId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MenuItemId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MenuItemId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MenuItemId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
