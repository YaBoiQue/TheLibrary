@model IEnumerable<TheWarehouse.Models.Inventory>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Timestamp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Employee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Receipt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supply)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Timestamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodeNavigation.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.IdEmployees)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Receipt.IdReceipts)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supply.IdSupplies)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdInventory">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdInventory">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdInventory">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
