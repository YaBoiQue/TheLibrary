@model IEnumerable<TheWarehouse.Models.Supply>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedTs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedTs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supplier)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupplyCategory)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedTs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedTs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supplier.SupplierId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplyCategory.SupplycategoryId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SupplyId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SupplyId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SupplyId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
